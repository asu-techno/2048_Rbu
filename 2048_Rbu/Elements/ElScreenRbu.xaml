<UserControl x:Class="_2048_Rbu.Elements.ElScreenRbu"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:gl="clr-namespace:System.Globalization;assembly=mscorlib"
      xmlns:indicators="clr-namespace:_2048_Rbu.Elements.Indicators"
      xmlns:mechs="clr-namespace:_2048_Rbu.Elements.Mechs"
      xmlns:control="clr-namespace:_2048_Rbu.Elements.Control"
      xmlns:local="clr-namespace:_2048_Rbu.Elements"
      xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
      mc:Ignorable="d"
      d:DataContext="{d:DesignInstance Type={x:Type local:ViewModelScreenRbu}, IsDesignTimeCreatable=False}">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../Resources/Styles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid Width="1920" Height="1020" Background="Silver" Name="ElGrid" VerticalAlignment="Center" HorizontalAlignment="Left">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <Menu Grid.Row="0" VerticalAlignment="Top" FontSize="14" BorderThickness="1" BorderBrush="Black">
            <MenuItem Name="Program" Header="Программа" Foreground="Black" BorderBrush="Black" BorderThickness="1,0" FontWeight="Bold" Height="30">
                <MenuItem Name="ChangeUser" Header="Сменить пользователя" Click="Login_OnClick"/>
                <Separator/>
                <MenuItem Name="Exit" Header="Выход" Click="Program_Closed"/>
            </MenuItem>
            <MenuItem Name="Settings" Header="Настройки" Foreground="Black" BorderBrush="Black" BorderThickness="1,0" FontWeight="Bold" Height="30" ToolTipService.ShowOnDisabled="True">
                <MenuItem.Style>
                    <Style TargetType="{x:Type MenuItem}">
                        <Setter Property="IsEnabled" Value="False"/>
                        <Setter Property="ToolTipService.IsEnabled" Value="True"/>
                        <Setter Property="ToolTip" Value='Текущий пользователь не имеет прав доступа'/>
                        <Style.Triggers>
                            <DataTrigger Binding="{Binding SettingPermit}" Value="True">
                                <Setter Property="IsEnabled" Value="True"/>
                                <Setter Property="ToolTipService.IsEnabled" Value="False"/>
                            </DataTrigger>
                        </Style.Triggers>
                    </Style>
                </MenuItem.Style>
                <MenuItem Name="RecipeBase" Header="База рецептов">
                    <MenuItem Header="Типы рецептов" Click="RecipeTypes_OnClick"></MenuItem>
                    <Separator/>
                    <MenuItem Header="Группы рецептов" Click="RecipeGroups_OnClick"></MenuItem>
                    <Separator/>
                    <MenuItem Header="Рецепты" Click="Recipes_OnClick"></MenuItem>
                </MenuItem>
                <Separator/>
                <MenuItem Name="MainSettings" Header="Общие настройки" Click="MainSettings_OnClick"/>
                <Separator/>
                <MenuItem Header="Материалы">
                    <MenuItem Header="Выбор материалов" Click="MatchingMaterials_OnClick"/>
                    <Separator/>
                    <MenuItem Header="Настройки масс емкостей" Click="WeightMaterials_OnClick"/>
                </MenuItem>
                <Separator/>
                <MenuItem Name="DosingSetting" Header="Настройки дозаторов">
                    <MenuItem Name="InertDosing" Header="Инертные материалы">
                        <MenuItem Name="Bunker1" Header="Бункер 1" Click="DosingBunker1_Click"/>
                        <Separator/>
                        <MenuItem Name="Bunker2" Header="Бункер 2" Click="DosingBunker2_Click"/>
                        <Separator/>
                        <MenuItem Name="Bunker3" Header="Бункер 3" Click="DosingBunker3_Click"/>
                        <!--<Separator/>
                        <MenuItem Name="Bunker4" Header="Бункер 4" Click="DosingBunker4_Click"/>-->
                    </MenuItem>
                    <MenuItem Name="CementDosing" Header="Цемент">
                        <MenuItem Name="Silos1" Header="Силос 1" Click="DosingSilo1_Click"/>
                        <Separator/>
                        <MenuItem Name="Seilos2" Header="Силос 2" Click="DosingSilo2_Click"/>
                    </MenuItem>
                    <MenuItem Name="AddDosing" Header="Химические добавки">
                        <MenuItem Name="AddTank1" Header="Емкость 1" Click="DosingAdditive1_Click"/>
                        <Separator/>
                        <MenuItem Name="AddTank2" Header="Емкость 2" Click="DosingAdditive2_Click"/>
                    </MenuItem>
                    <MenuItem Name="WaterDosing" Header="Вода">
                        <MenuItem Name="Water" Header="Вода" Click="DosingWater_Click"/>
                    </MenuItem>
                    <MenuItem Name="LimitValues" Header="Предельные значения дозирования" Click="MaterialSpeed_OnClick"/>
                </MenuItem>
                <Separator/>
                <MenuItem Name="VibratorSettings" Header="Настройки вибраторов" Click="VibratorSettings_OnClick"/>
                <Separator/>
                <MenuItem Name="AerationSettings" Header="Настройки аэраторов" Click="AerationSettings_OnClick"/>
                <Separator/>
                <MenuItem Name="Mode" Header="Режим работы механизмов" Click="Mode_OnClick"/>
                <Separator/>
                <MenuItem Name="Directory" Header="Настройки справочников">
                    <MenuItem Header="Типы материалов" Click="MaterialTypes_OnClick"></MenuItem>
                    <Separator/>
                    <MenuItem Header="Материалы" Click="Materials_OnClick"></MenuItem>
                    <Separator/>
                    <MenuItem Header="Типы емкостей" Click="ContainerTypes_OnClick"></MenuItem>
                    <Separator/>
                    <MenuItem Header="Емкости" Height="30" Click="Containers_OnClick"></MenuItem>
                    <Separator Visibility="Collapsed"/>
                    <MenuItem Header="Физическая модель" Visibility="Collapsed">
                        <MenuItem Header="Общие настройки" Click="CommonSettings_OnClick"/>
                        <Separator/>
                        <MenuItem Header="Источники дозирования" Click="DosingSources_OnClick"/>
                        <Separator/>
                        <MenuItem Header="Дозаторы" Click="Batchers_OnClick"/>
                    </MenuItem>
                </MenuItem>
                <Separator/>
                <MenuItem Name="Users" Header="Настройки пользователей" Click="User_OnClick" ToolTipService.ShowOnDisabled="True">
                    <MenuItem.Style>
                        <Style TargetType="{x:Type MenuItem}">
                            <Setter Property="IsEnabled" Value="False"/>
                            <Setter Property="ToolTipService.IsEnabled" Value="True"/>
                            <Setter Property="ToolTip" Value='Текущий пользователь не имеет прав доступа'/>
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding UserPermit}" Value="True">
                                    <Setter Property="IsEnabled" Value="True"/>
                                    <Setter Property="ToolTipService.IsEnabled" Value="False"/>
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </MenuItem.Style>
                </MenuItem>
            </MenuItem>
            <MenuItem Name="Archive" Header="Архивы" Foreground="Black" BorderBrush="Black" BorderThickness="1,0" FontWeight="Bold" Height="30">
                <MenuItem Name="Archiver" Header="Архиватор" Click="Archiver_OnClick"/>
                <!--<Separator/>
                <MenuItem Name="Graphics" Header="Архив работы" Click="Archive_OnClick"/>-->
                <Separator/>
                <MenuItem Name="GraphicsNew" Header="Архив работы" Click="ArchiveNew_OnClick"/>
                <Separator/>
                <MenuItem Name="EventsArchive" Header="Журнал событий" Click="EventsArchive_OnClick"/>
            </MenuItem>
            <MenuItem Name="Reports" Header="Отчеты" Foreground="Black" BorderBrush="Black" BorderThickness="1,0" FontWeight="Bold" Height="30">
                <MenuItem Name="RecipeReport" Header="Отчет по заданию" Click="RecipeReport_Click"/>
            </MenuItem>
            <MenuItem Name="Spalsh" Header="О программе" Foreground="Black" BorderBrush="Black" BorderThickness="1,0,2,0" FontWeight="Bold" Height="30" Click="Spalsh_OnClick"/>
        </Menu>
        <Image VerticalAlignment="Center" HorizontalAlignment="Center" Grid.Row="0" Margin="0" Width="160" Height="30" Source="/2048_Rbu;component/Images/Logo_ASU_Gray.png" Stretch="Fill" RenderOptions.BitmapScalingMode="HighQuality"/>
        <Label Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Width="160" Height="26" Margin="0,0,10,0" BorderBrush="Black" BorderThickness="1" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="14" FontWeight="Bold" Padding="0" Content="{Binding CurrentDateTime}" MouseLeave="UIElement_OnMouseLeave" MouseEnter="UIElement_OnMouseEnter"/>
        <Button Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Style="{StaticResource PressButton}" IsEnabled="{Binding LinkOk}" Content="Общий СТОП" FontSize="14" BorderThickness="1" BorderBrush="Black" Background="Salmon" Margin="0,0,175,0" Width="160" Height="26" Name="BtnStop" Click="BtnStop_OnClick"/>
