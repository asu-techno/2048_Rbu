<UserControl x:Class="_2048_Rbu.Elements.ElScreenRbu"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:indicators="clr-namespace:_2048_Rbu.Elements.Indicators"
      xmlns:mechs="clr-namespace:_2048_Rbu.Elements.Mechs"
      xmlns:control="clr-namespace:_2048_Rbu.Elements.Control"
      xmlns:local="clr-namespace:_2048_Rbu.Elements"
      xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
      mc:Ignorable="d"
      d:DataContext="{d:DesignInstance Type={x:Type local:ViewModelScreenRbu}, IsDesignTimeCreatable=False}">
    <Grid Width="1920" Height="1020" Background="Silver" Name="ElGrid" VerticalAlignment="Center" HorizontalAlignment="Left">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <Menu Grid.Row="0" VerticalAlignment="Top" FontSize="14" BorderThickness="1" BorderBrush="Black">
            <MenuItem Name="Archive" Header="Архивы" Foreground="Black" BorderBrush="Black" BorderThickness="1,0" FontWeight="Bold" Height="30">
                <MenuItem Name="Graphics" Header="Архив работы" Click="Archive_OnClick"/>
                <Separator/>
                <MenuItem Name="EventsArchive" Header="Журнал событий" Click="EventsArchive_OnClick"/>
            </MenuItem>
            <MenuItem Name="Settings" Header="Настройки" Foreground="Black" BorderBrush="Black" BorderThickness="1,0" FontWeight="Bold" Height="30">
                <MenuItem.Style>
                    <Style TargetType="{x:Type MenuItem}">
                        <Setter Property="IsEnabled" Value="True"/>
                        <Style.Triggers>
                            <DataTrigger Binding="{Binding LinkMessage}" Value="True">
                                <Setter Property="IsEnabled" Value="False"/>
                            </DataTrigger>
                        </Style.Triggers>
                    </Style>
                </MenuItem.Style>
                <MenuItem Name="Mode" Header="Режим работы механизмов" Click="Mode_OnClick"/>
                <Separator/>
                <MenuItem Name="DosingSetting" Header="Настройки дозаторов">
                    <MenuItem Name="InertDosing" Header="Инертные материалы">
                        <MenuItem Name="Bunker1" Header="Бункер 1" Click="DosingBunker1_Click"/>
                        <Separator/>
                        <MenuItem Name="Bunker2" Header="Бункер 2" Click="DosingBunker2_Click"/>
                        <Separator/>
                        <MenuItem Name="Bunker3" Header="Бункер 3" Click="DosingBunker3_Click"/>
                        <Separator/>
                        <MenuItem Name="Bunker4" Header="Бункер 4" Click="DosingBunker4_Click"/>
                    </MenuItem>
                    <MenuItem Name="CementDosing" Header="Цемент">
                        <MenuItem Name="Silos1" Header="Силос 1" Click="DosingSilo1_Click"/>
                        <Separator/>
                        <MenuItem Name="Seilos2" Header="Силос 2" Click="DosingSilo2_Click"/>
                    </MenuItem>
                    <MenuItem Name="AddDosing" Header="Химические добавки">
                        <MenuItem Name="AddTank1" Header="Емкость 1" Click="DosingAdditive1_Click"/>
                        <Separator/>
                        <MenuItem Name="AddTank2" Header="Емкость 2" Click="DosingAdditive2_Click"/>
                    </MenuItem>
                    <MenuItem Name="WaterDosing" Header="Вода">
                        <MenuItem Name="Water" Header="Вода" Click="DosingWater_Click"/>
                    </MenuItem>
                </MenuItem>
                <Separator/>
                <MenuItem Name="VibratorSettings" Header="Настройки вибраторов" Click="VibratorSettings_OnClick"/>
            </MenuItem>
            <MenuItem Name="Reports" Header="Отчеты" Foreground="Black" BorderBrush="Black" BorderThickness="1,0" FontWeight="Bold" Height="30">
                <MenuItem Name="RecipeReport" Header="Отчет по заданию" Click="RecipeReport_Click"/>
            </MenuItem>
            <MenuItem Name="Directory" Header="Справочник" Foreground="Black" BorderBrush="Black" BorderThickness="1,0,2,0" FontWeight="Bold" Height="30">
                <MenuItem Header="Типы материалов" Click="MaterialTypes_OnClick"></MenuItem>
                <Separator/>
                <MenuItem Header="Материалы" Click="Materials_OnClick"></MenuItem>
                <Separator/>
                <MenuItem Header="Типы емкостей" Click="ContainerTypes_OnClick"></MenuItem>
                <Separator/>
                <MenuItem Header="Емкости" Height="30" Click="Containers_OnClick"></MenuItem>
                <Separator/>
                <MenuItem Header="Типы рецептов" Click="RecipeTypes_OnClick"></MenuItem>
                <Separator/>
                <MenuItem Header="Группы рецептов" Click="RecipeGroups_OnClick"></MenuItem>
                <Separator/>
                <MenuItem Header="Рецепты" Click="Recipes_OnClick"></MenuItem>
                <Separator/>
                <MenuItem Header="Выбор материалов" Click="MatchingMaterials_OnClick"></MenuItem>
                <Separator/>
                <MenuItem Header="Физическая модель">
                    <MenuItem Header="Общие настройки" Click="CommonSettings_OnClick"/>
                    <Separator/>
                    <MenuItem Header="Источники дозирования" Click="DosingSources_OnClick"/>
                    <Separator/>
                    <MenuItem Header="Дозаторы" Click="Batchers_OnClick"/>
                </MenuItem>
            </MenuItem>
        </Menu>
        <Image VerticalAlignment="Center" HorizontalAlignment="Center" Grid.Row="0" Margin="0" Width="160" Height="30" Source="/2048_Rbu;component/Images/Logo_ASU_Gray.png" Stretch="Uniform" RenderOptions.BitmapScalingMode="Fant"/>
        <Label Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Width="160" Height="26" Margin="0,0,10,0" BorderBrush="Black" BorderThickness="1" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="14" FontWeight="Bold" Padding="0" Content="{Binding CurrentDateTime}" />
        <Button Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Content="Общий СТОП" FontSize="14" BorderThickness="1" BorderBrush="Black" FontWeight="Bold" Background="Salmon" Margin="0,0,175,0" Width="160" Height="26" Name="BtnStop" Click="BtnStop_OnClick">
            <Button.Style>
