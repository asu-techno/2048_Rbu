<UserControl x:Class="_2048_Rbu.Elements.Control.ElTabl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:_2048_Rbu.Elements.Control"
             mc:Ignorable="d" 
             d:DataContext="{d:DesignInstance Type={x:Type local:ViewModelTabl}, IsDesignTimeCreatable=False}"
             Height="187" Width="380" Background="Transparent">
    <Grid>

        <StackPanel>

            <TextBlock  FontFamily="{StaticResource Jost}"
                        FontWeight="Regular"
                        FontSize="14"
                        Text="Т Е К У Щ Е Е   З А Д А Н И Е"
                        HorizontalAlignment="Left"
                        Foreground="#676A70">
            </TextBlock>

            <StackPanel Orientation="Horizontal"
                        Height="25"
                        Margin="0,17,0,0"
                        VerticalAlignment="Top">
                
                <TextBlock Text="Название:"
                           FontFamily="{StaticResource Jost}"
                           FontWeight="Regular"
                           FontSize="16"
                           Foreground="#333538"
                           Width="140"
                           HorizontalAlignment="Left"
                           VerticalAlignment="Center">

                </TextBlock>
                
                <Border Width="240"
                        Background="#EEEEEF"
                        CornerRadius="5"
                        BorderThickness="1"
                        BorderBrush="#B8BBC0">
                    
                    <TextBlock Background="Transparent"
                               FontFamily="{StaticResource Jost}"
                               FontWeight="Regular"
                               FontSize="14"
                               Margin="15,0,0,0"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Center"
                               Foreground="#3B3C3D"
                               Text="{Binding SelTaskName, UpdateSourceTrigger=PropertyChanged}">

                    </TextBlock>
                </Border>
                
            </StackPanel>

            <StackPanel Orientation="Horizontal"
                        Height="25"
                        Margin="0,5,0,0">
                
                <TextBlock Text="Группа рецептов:"
                           FontFamily="{StaticResource Jost}"
                           FontWeight="Regular"
                           FontSize="16"
                           Foreground="#333538"
                           Width="140"
                           HorizontalAlignment="Left"
                           VerticalAlignment="Center">

                </TextBlock>
                
                <Border Width="240"
                        Background="#EEEEEF"
                        CornerRadius="5"
                        BorderThickness="1"
                        BorderBrush="#B8BBC0">
                    
                    <TextBlock Background="Transparent"
                               Text="{Binding SelTaskVolume, UpdateSourceTrigger=PropertyChanged}"
                               FontFamily="{StaticResource Jost}"
                               FontWeight="Regular"
                               FontSize="14"
                               Margin="15,0,0,0"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Center"
                               Foreground="#3B3C3D">

                    </TextBlock>
                </Border>
            </StackPanel>

            <StackPanel Orientation="Horizontal"
                        Height="25"
                        Margin="0,5,0,0">
                <TextBlock Text="Заказчик:"
                           FontFamily="{StaticResource Jost}"
                           FontWeight="Regular"
                           FontSize="16"
                           Foreground="#333538"
                           Width="140"
                           HorizontalAlignment="Left"
                           VerticalAlignment="Center">

                </TextBlock>
                <Border Width="240"
                        Background="#EEEEEF"
                        CornerRadius="5"
                        BorderThickness="1"
                        BorderBrush="#B8BBC0">
                    
                    <TextBlock Background="Transparent"
                               Text="{Binding SelTaskGroupName, UpdateSourceTrigger=PropertyChanged}"
                               FontFamily="{StaticResource Jost}"
                               FontWeight="Regular"
                               FontSize="14"
                               Margin="15,0,0,0"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Center"
                               Foreground="#3B3C3D">

                    </TextBlock>
                </Border>
            </StackPanel>

            <StackPanel Orientation="Horizontal"
                        Height="25"
                        Margin="0,5,0,0">
                <TextBlock Text="Задано:"
                           FontFamily="{StaticResource Jost}"
                           FontWeight="Regular"
                           FontSize="16"
                           Foreground="#333538"
                           Width="140"
                           HorizontalAlignment="Left"
                           VerticalAlignment="Center">

                </TextBlock>
                <Border Width="240"
                        Background="#EEEEEF"
                        CornerRadius="5"
                        BorderThickness="1"
                        BorderBrush="#B8BBC0">
                    
                    <TextBlock Background="Transparent"
                               Text="{Binding SelTask.Customer.Name, UpdateSourceTrigger=PropertyChanged}"
                               FontFamily="{StaticResource Jost}"
                               FontWeight="Regular"
                               FontSize="14"
                               Margin="15,0,0,0"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Center"
                               Foreground="#3B3C3D">

                    </TextBlock>
                </Border>
            </StackPanel>

            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right"
                        Margin="0,5,0,0">

                <Button ToolTip="Выгрузить текущий рецепт" 
                        Command="{Binding UnloadTask}"
                        Content="Выгрузить"
                        FontSize="16"
                        Width="240"
                        Height="30"
                        Margin="0,0,0,0"
                        Style="{StaticResource NewButton}"
                        Tag="{Binding TaskId}">
                </Button>
                
            </StackPanel>

        </StackPanel>

        
