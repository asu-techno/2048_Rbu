<UserControl x:Class="_2048_Rbu.Elements.Control.ElZamesDosing"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:_2048_Rbu.Elements.Control"
             mc:Ignorable="d" 
             Height="431" Width="380" Background="#E7EAF3"
             d:DataContext="{d:DesignInstance Type={x:Type local:ViewModelDosing}, IsDesignTimeCreatable=False}">

    <StackPanel>
        
        <!--Text = узел дозирования-->
        <TextBlock  
                  FontFamily="{StaticResource Jost}"
                  FontWeight="Regular"
                  FontSize="14"
                  HorizontalAlignment="Left"
                  Foreground="#878A91">
                  <Run Typography.CapitalSpacing="True">УЗЕЛ ДОЗИРОВАНИЯ</Run>
        </TextBlock>

        <!--StackPanel Замес-->
        <StackPanel Orientation="Horizontal"
                    Height="25"
                    Margin="0,15,0,0">
            
            <TextBlock Text="Замес:"
                       FontFamily="{StaticResource Jost}"
                       FontWeight="Regular"
                       FontSize="16"
                       Foreground="#333438"
                       Width="140"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center">

            </TextBlock>

            <Border Width="80"
                    Background="#F3F4F8"
                    CornerRadius="5"
                    BorderThickness="1"
                    BorderBrush="#B8BBC0">
                
                <TextBlock Background="Transparent"
                           FontFamily="{StaticResource Jost}"
                           FontWeight="Regular"
                           FontSize="14"
                           HorizontalAlignment="Center"
                           VerticalAlignment="Center"
                           Foreground="#333438"
                           Name="lbl_State" 
                           Text="{Binding Path=CurrentBatchNum, UpdateSourceTrigger=PropertyChanged}">

                </TextBlock>
                
            </Border>

            <TextBlock Width="80"
                       Background="Transparent"
                       Text="из"
                       VerticalAlignment="Center"
                       TextAlignment="Center"
                       FontSize="16"
                       FontFamily="{StaticResource Jost}"
                       FontWeight="Regular">

            </TextBlock>

            <Border Width="80"
                    Background="#F3F4F8"
                    CornerRadius="5"
                    BorderThickness="1"
                    BorderBrush="#B8BBC0">
                
                <TextBlock Background="Transparent"
                           FontFamily="{StaticResource Jost}"
                           FontWeight="Regular"
                           FontSize="14"
                           HorizontalAlignment="Center"
                           VerticalAlignment="Center"
                           Foreground="#333438"
                           Text="{Binding Path=BatchesQuantity, UpdateSourceTrigger=PropertyChanged}">
                    
                    <Label Name="LblSetBatchesQuantity"
                           VerticalAlignment="Top"
                           HorizontalAlignment="Center"
                           FontSize="14" 
                           ToolTip="Изменить количество замесов">
                        
                        <Label.InputBindings>
                            <MouseBinding Command="{Binding SetBatchesQuantity}" MouseAction="LeftClick"/>
                        </Label.InputBindings>
                        
                        <Label.Style>
                            <Style TargetType="{x:Type Label}">
                                <Setter Property="OverridesDefaultStyle" Value="True"/>
                                <Setter Property="SnapsToDevicePixels" Value="True"/>
                                <Setter Property="FontSize" Value="14"/>
                                
                                <Setter Property="Template">
                                    <Setter.Value>
                                        <ControlTemplate>
                                            <Border Name="Border">
                                                <TextBlock Text="{Binding BatchesQuantity}"
                                                           TextAlignment="Center" 
                                                           VerticalAlignment="Center"/>
                                            </Border>
                                            <ControlTemplate.Triggers>
                                                <Trigger Property="IsMouseOver" Value="False">
                                                    <Setter TargetName="Border" Property="Background" Value="#FFADD8E6"/>
                                                    <Setter TargetName="Border" Property="BorderBrush" Value="#FF000000"/>
                                                    <Setter Property="Foreground" Value="#FF000000"/>
                                                </Trigger>
                                                <Trigger Property="IsMouseOver" Value="True">
                                                    <Setter TargetName="Border" Property="Background" Value="#FFC9DEF5"/>
                                                    <Setter TargetName="Border" Property="BorderBrush" Value="#FF3399FF"/>
                                                    <Setter Property="Foreground" Value="#FF000000"/>
                                                </Trigger>
                                            </ControlTemplate.Triggers> 
                                        </ControlTemplate>
                                    </Setter.Value>
                                </Setter>
                                
                                <Setter Property="Visibility" Value="Collapsed"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding VisSetBatchesQuantity}" Value="True">
                                        <Setter Property="Visibility" Value="Visible"/>
                                    </DataTrigger>
                                </Style.Triggers>
                                
                            </Style>
                        </Label.Style>
                    </Label>
                </TextBlock>
            </Border>
        </StackPanel>

        <!--Дозирование-->
        <Grid Margin="0,15,0,0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="140"/>
                <ColumnDefinition Width="178"/>
                <ColumnDefinition Width="47"/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>

            <Grid.RowDefinitions>
                <RowDefinition Height="15"/>
                <RowDefinition Height="15"/>
            </Grid.RowDefinitions>

            <TextBlock Grid.Row="0" Grid.RowSpan="1"
                       Text="Дозирование:"
                       FontFamily="{StaticResource Jost}"
                       FontWeight="Regular"
                       FontSize="16"
                       Foreground="#333438"
                       Width="140"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center">
            </TextBlock>

            <ProgressBar  Grid.Row="0"
                          Grid.Column="1"
                          Width="178"
                          Height="5"
                          Style="{StaticResource ProgressBarHorizontal}"
                          Name="ProgressDosing" 
                          Value="{Binding Path=DosingProcess, UpdateSourceTrigger=PropertyChanged}"
                          >
            </ProgressBar>

            <TextBlock Grid.Row="1"
                       Grid.Column="1"
                       FontFamily="{StaticResource Jost}"
                       FontWeight="Regular"
                       FontSize="12"
                       Foreground="#018667"
                       HorizontalAlignment="Right"
                       VerticalAlignment="Center">

                <TextBlock.Style>
                    <Style TargetType="{x:Type TextBlock}">
                        <Style.Triggers>
                            <DataTrigger Binding="{Binding ElementName=ProgressDosing, Path=Value}">
                                <Setter Property="Text" Value="Выполняется"/>
                            </DataTrigger>
                            <DataTrigger Binding="{Binding ElementName=ProgressDosing, Path=Value}" Value="100">
                                <Setter Property="Text" Value="Выполнено"/>
                            </DataTrigger>
                        </Style.Triggers>
                    </Style>
                </TextBlock.Style>
                
            </TextBlock>

            <TextBlock Grid.Row="0"
                       Grid.Column="2"
                       Grid.RowSpan="2"
                       Text="{Binding ElementName=ProgressDosing, Path=Value, StringFormat={}{0:0}}"
                       FontFamily="{StaticResource Jost}"
                       FontWeight="Regular"
                       FontSize="27"
                       Foreground="#018667"
                       HorizontalAlignment="Right"
                       VerticalAlignment="Center">
            </TextBlock>

            <TextBlock Grid.Row="0"
                       Grid.Column="3"
                       Text="%"
                       FontFamily="{StaticResource Jost}"
                       FontWeight="Regular"
                       FontSize="14"
                       Foreground="#018667"
                       HorizontalAlignment="Center"
                       VerticalAlignment="Top">

            </TextBlock>

        </Grid>
        
        <!--ListView of Material-->

        <TextBlock Grid.Row="0"
