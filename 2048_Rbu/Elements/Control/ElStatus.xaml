                <StackPanel Orientation="Horizontal"
                            Width="100">

                    <Rectangle Margin="20,0,0,0" 
                               Width="10" 
                               Height="10" 
                               StrokeThickness="1" 
					           Stroke="#3B3C3D"
                               RadiusX="10" 
                               RadiusY="10">

                        <Rectangle.Style>
                            <Style TargetType="{x:Type Rectangle}">
                                <Setter Property="Fill" Value="#B2BBC9"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding StatusArray[5].StatusItem}" Value="True">
