<UserControl x:Class="_2048_Rbu.Elements.Control.ElStatus"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:_2048_Rbu.Elements.Control"
      mc:Ignorable="d"
      Width="240" Height="269" >

    <!--d:DataContext="{d:DesignInstance Type={x:Type local:ElStatus}, IsDesignTimeCreatable=False}"-->

    <Border Background="#D9D8DD"
            CornerRadius="3,3,3,3"
            BorderThickness="1"
            BorderBrush="#B8BBC0">

        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="19"/>
                <RowDefinition Height="20"/>
                <RowDefinition Height="3"/>
                <RowDefinition Height="20"/>
                <RowDefinition Height="3"/>
                <RowDefinition Height="20"/>
                <RowDefinition Height="3"/>
                <RowDefinition Height="20"/>
                <RowDefinition Height="3"/>
                <RowDefinition Height="20"/>
                
                <RowDefinition Height="20"/>
                <RowDefinition Height="3"/>
                <RowDefinition Height="20"/>
                <RowDefinition Height="3"/>
                <RowDefinition Height="20"/>
                <RowDefinition Height="3"/>
                <RowDefinition Height="20"/>
                <RowDefinition Height="3"/>
                <RowDefinition Height="20"/>
                <RowDefinition Height="3"/>
                <RowDefinition Height="20"/>

            </Grid.RowDefinitions>

            <TextBlock Text="СТАТУСЫ УЗЛОВ"
                       Grid.Row="1"
                       FontFamily="{StaticResource Jost}"
                       FontWeight="Regular"
                       FontSize="14"
                       Foreground="#676A70"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       Padding="20,0,0,0">
                
            </TextBlock>

            <TextBlock Text="Стоповые кнопки"
                       Grid.Row="3"
                       FontFamily="{StaticResource Jost}"
                       FontWeight="Regular"
                       FontSize="14"
                       Foreground="#3B3C3D"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       Padding="20,0,0,0">
            </TextBlock>

            <!--ШУ ИФ, ШУ К-->
            <StackPanel Orientation="Horizontal"
                        Grid.Row="5">

                <StackPanel Orientation="Horizontal" 
                            Width="100"
                            Background="Transparent">

                    <Rectangle Margin="20,0,0,0" 
                               Width="10" 
                               Height="10" 
                               StrokeThickness="1" 
                               Stroke="#3B3C3D"
                               RadiusX="10" 
                               RadiusY="10">

                        <Rectangle.Style>
                            <Style TargetType="{x:Type Rectangle}">
                                <Setter Property="Fill" Value="#B2BBC9"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding StatusArray[0].StatusItem}" Value="True">
                                        <Setter Property="Fill" Value="#19BF2B"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Rectangle.Style>

                    </Rectangle>

                    <TextBlock Text="ШУ К"
                           FontFamily="{StaticResource Jost}"
                           FontWeight="Regular"
                           FontSize="14"
                           Padding="10,0,0,0"
                           VerticalAlignment="Center"
                           Foreground="#3B3C3D">
                    </TextBlock>

                </StackPanel>

                <StackPanel Orientation="Horizontal" 
                            Width="100"
                            Background="Transparent">
                    
                    <Rectangle Margin="20,0,0,0" 
                           Width="10" 
                           Height="10" 
                           StrokeThickness="1" 
                           Stroke="#3B3C3D"
                           RadiusX="10" 
                           RadiusY="10">

                        <Rectangle.Style>
                            <Style TargetType="{x:Type Rectangle}">
                                <Setter Property="Fill" Value="#B2BBC9"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding StatusArray[1].StatusItem}" Value="True">
                                        <Setter Property="Fill" Value="#19BF2B"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Rectangle.Style>

                    </Rectangle>

                    <TextBlock Text="ШУ ИФ"
                           FontFamily="{StaticResource Jost}"
                           FontWeight="Regular"
                           FontSize="14"
                           Padding="10,0,0,0"
                           VerticalAlignment="Center"
                           Foreground="#3B3C3D">
                    </TextBlock>

                </StackPanel>
                
            </StackPanel>

            <!--ШУ Ц, ШУ Д-->
            <StackPanel Orientation="Horizontal"
                        Grid.Row="7">

                <StackPanel Orientation="Horizontal" 
                            Width="100"
                            Background="Transparent">

                    <Rectangle Margin="20,0,0,0" 
                               Width="10" 
                               Height="10" 
                               StrokeThickness="1" 
                               Stroke="#3B3C3D"
                               RadiusX="10" 
                               RadiusY="10">

                        <Rectangle.Style>
                            <Style TargetType="{x:Type Rectangle}">
                                <Setter Property="Fill" Value="#B2BBC9"/>
                                <Style.Triggers>
