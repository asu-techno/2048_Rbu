                    <TextBlock Text="ШУ Д"
                           FontFamily="{StaticResource Jost}"
                           FontWeight="Regular"
                           FontSize="14"
                           Padding="10,0,0,0"
                           VerticalAlignment="Center"
                           Foreground="#3B3C3D"
