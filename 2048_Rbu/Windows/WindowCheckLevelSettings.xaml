<Window x:Class="_2048_Rbu.Windows.WindowCheckLevelSettings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
        xmlns:classes="clr-namespace:_2048_Rbu.Classes.ViewModel"
        xmlns:settings="clr-namespace:_2048_Rbu.Elements.Settings"
        mc:Ignorable="d" 
        d:DataContext="{d:DesignInstance Type={x:Type classes:CheckLevelSettingsViewModel}, IsDesignTimeCreatable = False}"
        Background="LightGray" SizeToContent="WidthAndHeight" Title="{Binding NameLevel}" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid Background="LightGray" Margin="5">
        <settings:ElCheckLevelSettings x:Name="ElCheckLevelSettings"/>
    </Grid>
</Window>
