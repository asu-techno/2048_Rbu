<Application x:Class="_2048_Rbu.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:styles="clr-namespace:Styles;assembly=Styles"
             xmlns:local="clr-namespace:_2048_Rbu"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/NewButton.xaml"/>
                <ResourceDictionary Source="Resources/AckButton.xaml"/>
                <ResourceDictionary Source="Resources/ProgressBar.xaml"/>
                <ResourceDictionary Source="Resources/StopButton.xaml"/>
                <ResourceDictionary Source="Resources/QueueButton.xaml"/>
                <ResourceDictionary Source="/Styles;component/Themes/Styles/DataGrid.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            <FontFamily x:Key="Jost">
                pack://application:,,,/Fonts/#Jost
            </FontFamily>
            
        </ResourceDictionary>
        
    </Application.Resources>
</Application>
